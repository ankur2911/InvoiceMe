{
  "name": "Flutter InvoiceMe",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "flutter doctor && flutter pub get",
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "Flutter Web Preview",
      "onAutoForward": "openBrowser"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.flutter",
        "Dart-Code.dart-code"
      ]
    }
  }
}
